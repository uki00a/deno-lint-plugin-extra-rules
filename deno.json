{
  "name": "@uki00a/deno-lint-plugin-extra-rules",
  "version": "0.1.0",
  "exports": {
    ".": "./mod.ts"
  },
  "tasks": {
    "check": "deno fmt --check && deno lint",
    "test": "deno test --allow-read=. test.ts"
  },
  "lint": {
    "rules": {
      "include": [
        "no-console"
      ]
    }
  },
  "exclude": ["./tmp"],
  "publish": {
    "exclude": [
      "./testdata",
      "./**/*_test.ts",
      "./**/test.ts",
      ".github/",
      "./.*"
    ]
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.11"
  }
}
